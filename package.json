{
    "name": "vscode-arm-params-generator",
    "displayName": "vscode-arm-params-generator",
    "description": "Generate ARM parameters from an existing ARM file",
    "version": "0.0.1",
    "publisher": "wilfriedwoivre",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Azure",
        "ARM"
    ],
    "activationEvents": [
        "onCommand:arm.generateParameterFile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "arm.generateParameterFile",
                "title": "Azure ARM : Generate parameters file"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "arm.generateParameterFile",
                    "when": "editorLangId == json"
                }
            ],
            "editor/context": [
                {
                    "command": "arm.generateParameterFile",
                    "when": "editorLangId == json"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
